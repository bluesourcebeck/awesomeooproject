<%= form_for @user, url: oo_path(@user), html:{method: 'PATCH'} do |f| %>
<fieldset>
<div>
    <%= f.label :name %>
    <%= f.text_field :name %>
</div>
<div>
    <%= f.label :story %>
    <%= f.text_field :story %>
</div>
<div>
    <%= f.label :avatar %>
    <%= f.file_field :avatar %>
</div>
</fieldset>
<div> <%=f.submit "Add", :class => 'btn btn-default btn-lg'%>
<%end%>